{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "A production-grade API Gateway built with TypeScript, Node.js, PostgreSQL, and Redis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:typeorm": "typeorm-ts-node-commonjs migration:run -d src/database/typeorm-cli.config.ts",
    "migrate:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/typeorm-cli.config.ts",
    "migrate:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/typeorm-cli.config.ts",
    "seed": "ts-node src/database/seed.ts",
    "load-test": "artillery run load-tests/artillery.yml",
    "docker:build": "docker build -t api-gateway .",
    "docker:run": "docker run -p 3000:3000 api-gateway",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "clean": "rm -rf dist node_modules",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "api-gateway",
    "typescript",
    "nodejs",
    "postgresql",
    "redis",
    "load-balancing",
    "rate-limiting",
    "authentication",
    "analytics",
    "monitoring"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/ioredis": "^4.28.10",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.2",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.25",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.1",
    "@types/pg": "^8.15.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "artillery": "^2.0.0-27",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/api-gateway.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/api-gateway/issues"
  },
  "homepage": "https://github.com/your-username/api-gateway#readme"
}
